{% extends 'Template.html.twig' %}

{% block title %}Seance{% endblock %}

{% block body %}
    <h1>Seance</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ seance.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ seance.nom }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ seance.date ? seance.date|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>HeureDeb</th>
                <td>{{ seance.heureDeb ? seance.heureDeb|date('H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>HeureFin</th>
                <td>{{ seance.heureFin ? seance.heureFin|date('H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_seance_index') }}">back to list</a>

    <a href="{{ path('app_seance_edit', {'id': seance.id}) }}">edit</a>

    {{ include('seance/_delete_form.html.twig') }}
{% endblock %}
